<h1>New Photo</h1>


<%= form_for :photo, url: photos_path do |f| %>


   <%= f.label :title %>
   <%= f.text_field :title %>

   <%= f.label :image %>
   <%= f.file_field :image %>

   <%= f.label :caption %>
   <%= f.text_field :text %>

   <%= f.label :tags %>
   <%= f.text_field :tag_names %>
 
   <%= f.submit 'add photo'%>
  <% end %>

